<%# <div class="container"> %>
  <div class="log_in">

    <h1>Log in</h1>
    <%= form_with(scope: :session, url: sessions_path, local: true) do |f| %>

      <div>
        <%= f.label :email %>
        <br>
        <%= f.email_field :email %>
      </div>

      <div>
        <%= f.label :password %>
        <br>
        <%= f.password_field :password %>
      <div>

        <%= f.submit "Log in", class: "btn btn-default" %>
      </div>
    <% end %>
  </div>
<%# </div> %>
